    "@types/koa-session-minimal": "^3.0.5",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^2.0.3",
    "async": "^3.1.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
